    "@react-navigation/native": "^6.1.18",
    "@reduxjs/toolkit": "^2.7.0",
    "@rneui/themed": "^4.0.0-rc.8",
    "@supabase/supabase-js": "^2.49.4",
    "@types/react": "~18.3.12",
    "burnt": "^0.12.2",
    "expo": "~52.0.46",
    "expo-av": "~15.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.8",
    "expo-device": "~7.0.3",
